units real
atom_style full
boundary p p p

# read_data ../../vle/datafile
read_data ../../TIP4P/2005/1H2O
replicate 8 8 8


pair_style deepmd graph-compress.pb
pair_coeff * * 

bond_style none
angle_style none

neighbor 2.0 bin
neigh_modify every 10 delay 0 check no

variable T equal 298.15
velocity all create ${T} 1488 dist uniform

fix 1 all nvt temp ${T} ${T} 500.0

timestep 0.5

group o type 1
# compute binchunk o chunk/atom bin/1d z center
# fix density_prof1 o ave/chunk 100 10 1000 binchunk density/number file density_prof.txt

thermo_style custom step temp etotal pe ke ecoul evdwl elong etail density lz pyy pzz
thermo 100
run 1000